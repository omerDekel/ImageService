@model WebApplication2.Models.ImageWeb

@{
    ViewBag.Title = "ImageWebView";
}

<h2>Image Web</h2>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Status)

        </th>

    </tr>
    <tr>

        <td>@Model.Status</td>

    </tr>

    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PicturesNumber)

        </th>

    </tr>
    <tr>
        <td>@Model.PicturesNumber</td>
    </tr>
</table>
<table class="table">
    <tr>
        @Html.DisplayNameFor(model => model.Students)
    </tr>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Students[0].FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Students[0].LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Students[0].ID)
        </th>
    </tr>
    @foreach (var item in Model.Students)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
        </tr>
    }

</table>